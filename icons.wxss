@font-face {
  font-family: "remixicon";
  src: url("data:font/woff2;charset=utf-8;base64,")
    format("woff2");

  font-display: swap;
}

[class^="ri-"],
[class*="ri-"] {
  font-family: "remixicon" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.ri-lg {
  /* font-size: 1.3333em;
  line-height: 0.75em;
  vertical-align: -0.0667em; */
  font-size: 1.5em;
}
.ri-xl {
  font-size: 1.5em;
  line-height: 0.6666em;
  vertical-align: -0.075em;
}
.ri-xxs {
  font-size: 0.5em;
}
.ri-xs {
  font-size: 0.75em;
}
.ri-sm {
  font-size: 0.875em;
}
.ri-1x {
  font-size: 1em;
}
.ri-2x {
  font-size: 2em;
}
.ri-3x {
  font-size: 3em;
}
.ri-4x {
  font-size: 4em;
}
.ri-5x {
  font-size: 5em;
}
.ri-6x {
  font-size: 6em;
}
.ri-7x {
  font-size: 7em;
}
.ri-8x {
  font-size: 8em;
}
.ri-9x {
  font-size: 9em;
}
.ri-10x {
  font-size: 10em;
}
.ri-fw {
  text-align: center;
  width: 1.25em;
}

.ri-repeat-2-fill:before {
  content: "\f071";
}
.ri-repeat-one-fill:before {
  content: "\f075";
}
.ri-order-play-fill:before {
  content: "\efb6";
}
.ri-play-fill:before {
  content: "\f00a";
}
.ri-pause-fill:before {
  content: "\efd7";
}
.ri-rewind-fill:before {
  content: "\f085";
}
.ri-speed-fill:before {
  content: "\f176";
}
.ri-menu-fill:before {
  content: "\ef3b";
}
.ri-close-fill:before {
  content: "\eb98";
}
.ri-restart-fill:before {
  content: "\f07f";
}
.ri-restart-line:before {
  content: "\f080";
}
.ri-pause-mini-fill:before {
  content: "\efd9";
}
